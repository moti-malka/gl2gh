{
  "dashboard": {
    "title": "Discovery Agent - Overview",
    "tags": ["discovery-agent", "gitlab", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "API Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(gitlab_api_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}} - {{status_code}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "API Rate Limit Remaining",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "gitlab_api_rate_limit_remaining",
            "legendFormat": "Remaining",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Remaining Calls"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [100], "type": "lt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "API Rate Limit Low",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 3,
        "title": "API Latency (p95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(gitlab_api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(gitlab_api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(discovery_errors_total[5m])",
            "legendFormat": "{{type}} - {{step}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Discovery Progress",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "discovery_projects_total",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "orientation": "auto",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 6,
        "title": "Groups Discovered",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 16},
        "targets": [
          {
            "expr": "discovery_groups_total",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "orientation": "auto",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 7,
        "title": "Total Errors",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "sum(discovery_errors_total)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Discovery Duration",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 16},
        "targets": [
          {
            "expr": "discovery_duration_seconds",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "orientation": "auto",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 9,
        "title": "Projects by Complexity",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "sum by (complexity) (discovery_projects_total)",
            "legendFormat": "{{complexity}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 10,
        "title": "Active Operations",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "active_operations",
            "legendFormat": "{{operation_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Active Count"},
          {"format": "short"}
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
    }
  }
}
